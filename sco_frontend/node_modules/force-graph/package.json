{
  "name": "force-graph",
  "version": "1.42.9",
  "description": "2D force-directed graph rendered on HTML5 canvas",
  "unpkg": "dist/force-graph.min.js",
  "main": "dist/force-graph.common.js",
  "module": "dist/force-graph.module.js",
  "types": "src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasturiano/force-graph.git"
  },
  "homepage": "https://github.com/vasturiano/force-graph",
  "keywords": [
    "2d",
    "force",
    "simulation",
    "graph",
    "canvas",
    "d3"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "http://bl.ocks.org/vasturiano"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vasturiano/force-graph/issues"
  },
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -w -c rollup.config.dev.js",
    "prepare": "npm run build"
  },
  "files": [
    "dist/**/*",
    "example/**/*",
    "src/**/*"
  ],
  "dependencies": {
    "@tweenjs/tween.js": "18",
    "accessor-fn": "1",
    "bezier-js": "3 - 6",
    "canvas-color-tracker": "1",
    "d3-array": "1 - 3",
    "d3-drag": "2 - 3",
    "d3-force-3d": "2 - 3",
    "d3-scale": "1 - 4",
    "d3-scale-chromatic": "1 - 3",
    "d3-selection": "2 - 3",
    "d3-zoom": "2 - 3",
    "index-array-by": "1",
    "kapsule": "^1.13",
    "lodash.throttle": "4"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "postcss": "^8.4.12",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-dts": "^4.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.6.3"
  }
}
