{
  "name": "d3-force-3d",
  "version": "3.0.3",
  "description": "Force-directed graph layout in 1D, 2D or 3D using velocity Verlet integration.",
  "homepage": "https://github.com/vasturiano/d3-force-3d",
  "repository": {
    "type": "git",
    "url": "https://github.com/vasturiano/d3-force-3d.git"
  },
  "keywords": [
    "d3",
    "d3-module",
    "layout",
    "network",
    "graph",
    "force",
    "verlet",
    "infovis",
    "3d"
  ],
  "license": "MIT",
  "author": {
    "name": "Vasco Asturiano",
    "url": "https://bl.ocks.org/vasturiano"
  },
  "type": "module",
  "files": [
    "src/**/*.js",
    "dist/**/*.js"
  ],
  "module": "src/index.js",
  "main": "src/index.js",
  "jsdelivr": "dist/d3-force-3d.min.js",
  "unpkg": "dist/d3-force-3d.min.js",
  "exports": {
    "umd": "./dist/d3-force-3d.min.js",
    "default": "./src/index.js"
  },
  "sideEffects": false,
  "dependencies": {
    "d3-binarytree": "^0.2.0",
    "d3-dispatch": "1 - 3",
    "d3-octree": "^0.2.0",
    "d3-quadtree": "1 - 3",
    "d3-timer": "1 - 3"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.0",
    "eslint": "^7.28.0",
    "mocha": "^9.0.0",
    "rollup": "^2.51.1",
    "rollup-plugin-terser": "^7.0.2",
    "tape": "^5.2.2"
  },
  "scripts": {
    "test": "mocha 'test/**/*-test.js' && eslint src test",
    "prepare": "rm -rf dist && yarn test && rollup -c"
  },
  "engines": {
    "node": ">=12"
  }
}
